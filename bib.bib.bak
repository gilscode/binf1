
@article{alvarez-ramirezTheoreticalDerivationMonod2019,
  title = {A Theoretical Derivation of the Monod Equation with a Kinetics Sense},
  author = {{Alvarez-Ramirez}, Jose and Meraz, M. and {Jaime Vernon-Carter}, E.},
  year = {2019},
  month = oct,
  volume = {150},
  pages = {107305},
  issn = {1369703X},
  doi = {10.1016/j.bej.2019.107305},
  abstract = {The Monod equation has been extensively used for modeling microbial growth, but has no mechanistic basis and its derivation is purely empirical. In this work an approach was developed for deriving the Monod equation based on a simple kinetics scheme. The microbial growth process considered involved two consecutive steps, i.e., (i) the formation of a substrate-cell ``complex'', and (ii) the metabolism of the substrate for cell reproduction. By departing from the law of mass action, the approach uses a singular perturbation method for derivation of the differential equations governing the microbial growth dynamics. Within this context, a Monod equation corrected by a quadratic term induced by auto-catalyst effects was obtained. The proposed model was validated with experimental data reported in the literature for the degradation of benzene, phenol and toluene by P. putida.},
  file = {C\:\\Users\\Gil\\Zotero\\storage\\RPW7A9A8\\Alvarez-Ramirez et al. - 2019 - A theoretical derivation of the monod equation wit.pdf},
  journal = {Biochemical Engineering Journal},
  language = {en}
}

