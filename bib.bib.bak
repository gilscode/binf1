@article{robertsDirecttoConsumerGeneticTesting2013,
  title = {Direct-to-{{Consumer Genetic Testing}} and {{Personal Genomics Services}}: {{A Review}} of {{Recent Empirical Studies}}},
  shorttitle = {Direct-to-{{Consumer Genetic Testing}} and {{Personal Genomics Services}}},
  author = {Roberts, J. Scott and Ostergren, Jenny},
  year = {2013},
